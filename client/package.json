{
  "name": "front",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "prepare": "npm run styles",
    "start": "turbo dev styles:watch",
    "dev": "vite --host",
    "build": "vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "tsc": "tsc --noEmit",
    "preview": "vite preview --host",
    "storybook": "start-storybook",
    "build-storybook": "build-storybook -s public",
    "format": "prettier --write -l .",
    "styles": "typed-scss-modules 'src/**/*.{css,scss}'",
    "styles:watch": "typed-scss-modules -w 'src/**/*.{css,scss}'"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
