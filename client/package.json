    "@hookform/resolvers": "^3.3.2",
    "@react-input/mask": "^1.0.20",
    "@reduxjs/toolkit": "^1.9.7",
    "axios": "^1.5.1",
    "card-validator": "^8.1.1",
    "clsx": "^2.0.0",
    "flickity": "^2.3.0",
    "radash": "^11.0.0",
    "react": "^18.2.0",
    "react-confirm-alert": "^3.0.6",
    "react-credit-cards-2": "^1.0.1",
