  "devDependencies": {
    "@types/react": "^18.2.36",
    "@types/react-dom": "^18.2.14",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitejs/plugin-react-swc": "^3.4.1",
    "browserslist-to-esbuild": "^1.2.0",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^9.0.0",
