{
  "name": "next-app",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "engines": {
    "node": "^18 || >=20",
    "pnpm": ">=8"
  },
  "scripts": {
    "build": "next build",
    "clear:styles": "rm -rf src/**/*.{css,scss}.d.ts",
    "dev": "next dev",
    "format": "prettier -wl --cache src db",
    "lint": "turbo lint:code lint:styles",
    "lint:fix": "pnpm lint -- --fix",
    "lint:code": "eslint --cache",
    "lint:styles": "stylelint --cache 'src/**/*.{css,scss}'",
    "prepare": "turbo styles",
    "migrate": "sequelize db:migrate && sequelize db:seed:all",
    "start": "next start",
    "typecheck": "tsc --noEmit",
    "styles": "typed-scss-modules 'src/**/*.{css,scss}' 1> /dev/null",
    "styles:watch": "pnpm styles -w"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.5.1",
    "@fortawesome/free-regular-svg-icons": "^6.5.1",
    "@fortawesome/free-solid-svg-icons": "^6.5.1",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@hookform/resolvers": "^3.3.4",
    "@react-input/mask": "^1.0.21",
    "@reduxjs/toolkit": "^2.2.1",
    "axios": "^1.6.7",
    "bcrypt": "^5.1.1",
    "card-validator": "^9.1.0",
    "clsx": "^2.1.0",
    "dotenv": "^16.4.5",
    "fast-deep-equal": "^3.1.3",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.1.3",
    "next": "^14.1.0",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.9.9",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "radash": "^12.0.0",
    "react": "^18.2.0",
    "react-confirm-alert": "^3.0.6",
    "react-credit-cards-2": "^1.0.2",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.50.1",
    "react-rating": "^2.0.5",
    "react-redux": "^9.1.0",
    "react-spinners": "^0.13.8",
    "react-toastify": "^10.0.4",
    "sequelize": "^6.37.1",
    "sharp": "^0.33.2",
    "socket.io": "^4.7.4",
    "socket.io-client": "^4.7.4",
    "swiper": "^11.0.6",
    "yet-another-react-lightbox": "^3.17.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.1.1",
    "@next/bundle-analyzer": "^14.1.0",
    "@next/eslint-plugin-next": "^14.1.0",
    "@types/bcrypt": "^5.0.2",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.11.19",
    "@types/node-cron": "^3.0.11",
    "@types/nodemailer": "^6.4.14",
    "@types/pg": "^8.11.0",
    "@types/react": "^18.2.57",
    "@types/react-dom": "^18.2.19",
    "@types/sequelize": "^4.28.20",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "prettier": "^3.2.5",
    "sass": "^1.71.1",
    "sequelize-cli": "^6.6.2",
    "stylelint": "^16.2.1",
    "stylelint-config-clean-order": "^5.4.1",
    "stylelint-config-prettier-scss": "^1.0.0",
    "stylelint-config-standard-scss": "^13.0.0",
    "stylelint-csstree-validator": "^3.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.8.0",
    "stylelint-group-selectors": "^1.0.10",
    "stylelint-plugin-defensive-css": "^1.0.2",
    "stylelint-plugin-logical-css": "^1.0.0",
    "turbo": "^1.12.4",
    "typed-scss-modules": "^8.0.0",
    "typescript": "^5.3.3",
    "typescript-eslint": "^7.0.2"
  },
  "pnpm": {
    "patchedDependencies": {
      "fast-deep-equal@3.1.3": "patches/fast-deep-equal@3.1.3.patch",
      "stylelint-csstree-validator@3.0.0": "patches/stylelint-csstree-validator@3.0.0.patch",
      "@typescript-eslint/eslint-plugin@7.0.2": "patches/@typescript-eslint__eslint-plugin@7.0.2.patch"
    }
  }
}
