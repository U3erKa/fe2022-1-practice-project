{
  "name": "next-app",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "next build",
    "clear:styles": "rm -rf src/**/*.{css,scss}.d.ts",
    "dev": "next dev",
    "format": "prettier --write -l src db",
    "lint": "turbo lint:code lint:styles",
    "lint:fix": "turbo lint:code lint:styles -- --fix",
    "lint:code": "next lint",
    "lint:styles": "stylelint 'src/**/*.{css,scss}'",
    "prepare": "turbo styles",
    "migrate": "sequelize db:migrate && sequelize db:seed:all",
    "start": "next start",
    "typecheck": "tsc --noEmit",
    "styles": "typed-scss-modules 'src/**/*.{css,scss}'",
    "styles:watch": "turbo styles -- -w"
  },
