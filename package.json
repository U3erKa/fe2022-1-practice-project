{
  "name": "next-app",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "engines": {
    "node": "^18 || >=20",
    "pnpm": ">=8"
  },
  "scripts": {
    "build": "next build",
    "clear:styles": "rm -rf src/**/*.{css,scss}.d.ts",
    "dev": "next dev",
    "format": "prettier -wl --cache src db",
    "lint": "turbo lint:code lint:styles",
    "lint:fix": "pnpm lint -- --fix",
    "lint:code": "next lint --cache",
    "lint:styles": "stylelint --cache 'src/**/*.{css,scss}'",
    "prepare": "turbo styles",
    "migrate": "sequelize db:migrate && sequelize db:seed:all",
    "start": "next start",
    "typecheck": "tsc --noEmit",
    "styles": "typed-scss-modules 'src/**/*.{css,scss}'",
    "styles:watch": "pnpm styles -w"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.5.1",
    "@fortawesome/free-regular-svg-icons": "^6.5.1",
    "@fortawesome/free-solid-svg-icons": "^6.5.1",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@hookform/resolvers": "^3.3.2",
    "@react-input/mask": "^1.0.20",
    "@reduxjs/toolkit": "^2.0.1",
    "axios": "^1.6.2",
    "bcrypt": "^5.1.1",
    "card-validator": "^9.0.0",
    "clsx": "^2.0.0",
    "dotenv": "^16.3.1",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.0.3",
    "next": "^14.0.4",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.9.7",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "radash": "^11.0.0",
    "react": "^18.2.0",
    "react-confirm-alert": "^3.0.6",
    "react-credit-cards-2": "^1.0.1",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.49.2",
    "react-rating": "^2.0.5",
    "react-redux": "^9.0.4",
    "react-spinners": "^0.13.8",
    "react-toastify": "^9.1.3",
    "sequelize": "^6.35.2",
    "socket.io": "^4.7.2",
    "socket.io-client": "^4.7.2",
    "swiper": "^11.0.5",
    "yet-another-react-lightbox": "^3.15.6",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/bcrypt": "^5.0.2",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.5",
    "@types/node-cron": "^3.0.11",
    "@types/nodemailer": "^6.4.14",
    "@types/pg": "^8.10.9",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@types/sequelize": "^4.28.19",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "prettier": "^3.1.1",
    "sass": "^1.69.5",
    "sequelize-cli": "^6.6.2",
    "stylelint": "^15.11.0",
    "stylelint-config-clean-order": "^5.2.0",
    "stylelint-config-prettier-scss": "^1.0.0",
    "stylelint-config-standard-scss": "^11.1.0",
    "stylelint-csstree-validator": "^3.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.7.0",
    "stylelint-group-selectors": "^1.0.10",
    "stylelint-plugin-defensive-css": "^0.10.2",
    "stylelint-plugin-logical-css": "^0.13.2",
    "turbo": "^1.11.2",
    "typed-scss-modules": "^8.0.0",
    "typescript": "^5.3.3"
  }
}
