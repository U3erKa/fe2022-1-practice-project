{
  "name": "next-app",
  "version": "1.0.0",
  "type": "module",
  "main": "src/server/",
  "private": true,
  "engines": {
    "node": "^18 || >=20",
    "pnpm": ">=8"
  },
  "scripts": {
    "build": "next build",
    "clear:styles": "rm -rf src/**/*.{css,scss}.d.ts",
    "dev:app": "next dev",
    "dev:server": "tsx .",
    "dev": "turbo dev:app dev:server",
    "format": "prettier -wl --cache src db",
    "lint:code": "eslint src --cache",
    "lint:fix": "pnpm lint -- --fix",
    "lint:styles": "stylelint --cache 'src/**/*.{css,scss}'",
    "lint": "turbo lint:code lint:styles",
    "migrate": "sequelize db:migrate && sequelize db:seed:all",
    "prepare": "turbo styles 1> /dev/null",
    "start": "next start",
    "styles:watch": "pnpm styles -w 1> /dev/null",
    "styles": "typed-scss-modules 'src/**/*.{css,scss}'",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.5.2",
    "@fortawesome/free-regular-svg-icons": "^6.5.2",
    "@fortawesome/free-solid-svg-icons": "^6.5.2",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@hookform/resolvers": "^3.3.4",
    "@react-input/mask": "^1.1.1",
    "@reduxjs/toolkit": "^2.2.3",
    "axios": "^1.6.8",
    "bcrypt": "^5.1.1",
    "card-validator": "^9.1.0",
    "clsx": "^2.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "fast-deep-equal": "^3.1.3",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.3.1",
    "next": "^14.2.1",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.9.13",
    "pg": "^8.11.5",
    "pg-hstore": "^2.3.4",
    "radash": "^12.1.0",
    "react": "^18.2.0",
    "react-confirm-alert": "^3.0.6",
    "react-credit-cards-2": "^1.0.2",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.51.3",
    "react-rating": "^2.0.5",
    "react-redux": "^9.1.0",
    "react-spinners": "^0.13.8",
    "react-toastify": "^10.0.5",
    "sequelize": "^6.37.2",
    "sharp": "^0.33.3",
    "socket.io": "^4.7.5",
    "socket.io-client": "^4.7.5",
    "swiper": "^11.1.1",
    "yet-another-react-lightbox": "^3.17.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.2.1",
    "@next/bundle-analyzer": "^14.2.1",
    "@next/eslint-plugin-next": "^14.2.1",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.12.7",
    "@types/node-cron": "^3.0.11",
    "@types/nodemailer": "^6.4.14",
    "@types/pg": "^8.11.5",
    "@types/react": "^18.2.78",
    "@types/react-dom": "^18.2.25",
    "@types/sequelize": "^4.28.20",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "prettier": "^3.2.5",
    "sass": "^1.75.0",
    "sequelize-cli": "^6.6.2",
    "stylelint": "^16.3.1",
    "stylelint-config-clean-order": "^5.4.2",
    "stylelint-config-prettier-scss": "^1.0.0",
    "stylelint-config-standard-scss": "^13.1.0",
    "stylelint-csstree-validator": "^3.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.8.0",
    "stylelint-group-selectors": "^1.0.10",
    "stylelint-plugin-defensive-css": "^1.0.3",
    "stylelint-plugin-logical-css": "^1.2.0",
    "tsx": "^4.7.2",
    "turbo": "^1.13.2",
    "typed-scss-modules": "^8.0.1",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.6.0"
  },
  "pnpm": {
    "patchedDependencies": {
      "fast-deep-equal@3.1.3": "patches/fast-deep-equal@3.1.3.patch",
      "stylelint-csstree-validator@3.0.0": "patches/stylelint-csstree-validator@3.0.0.patch"
    }
  }
}
