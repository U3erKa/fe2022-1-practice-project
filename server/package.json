{
  "name": "todo-list",
  "version": "0.0.0",
  "private": true,
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "prestart": "npm run build",
    "start": "turbo dev build:watch",
    "dev": "node --trace-deprecation --experimental-specifier-resolution node .",
    "build": "swc src/ -d dist/ --copy-files",
    "build:watch": "swc src/ -d dist/ --copy-files --watch",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "tsc": "tsc --noEmit",
    "format": "prettier --write -l .",
    "prepare": "npm run build"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^7.6.4",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.2",
    "nodemailer": "^6.9.7",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.34.0",
    "socket.io": "^4.7.2",
    "uuid": "^9.0.0",
    "yup": "^1.3.2"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.95",
    "@types/bcrypt": "^5.0.1",
    "@types/express": "^4.17.20",
    "@types/jsonwebtoken": "^9.0.4",
    "@types/multer": "^1.4.9",
    "@types/node": "^18.18.8",
    "@types/node-cron": "^3.0.10",
    "@types/nodemailer": "^6.4.13",
    "@types/sequelize": "^4.28.17",
    "@types/uuid": "^9.0.6",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "chokidar": "^3.5.3",
    "eslint": "^8.53.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.29.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "sequelize-cli": "^6.6.2",
    "turbo": "^1.10.16",
    "typescript": "^5.2.2"
  }
}
